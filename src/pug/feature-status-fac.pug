//- feature-status-lab
extends _layout.pug

block link
    - var path = 'feature-status-fac'

block main
    - 
      var status_group1 = [ 'PROMOTED', 'BUILT', 'FAILED' ]
      var status_group2 = []

    .main__header
        .main__zone.main__zone--fac
            .zone-icon.i.fas.fa-city
            .zone-name Factory
        .main__title Display feature's status by feature store's name
        .main__breadcrumb
            span Feature Status - 
                span.zone-name fac
            span /
            a(href="/") Choc-BERI
    .main__content.main__content--feature-status
        .feature-summary
            .strip
                .strip__title FEATURE STORES
                .strip__content
                    span.strip__feature-store-name.strip__feature-store-name--active all
                    span.strip__feature-store-name fixedservices
                    span.strip__feature-store-name mobile
            #fixedservices.feature-store
                .feature-store__header Feature store - fixedservices
                .feature-store__content

                    .feature-status.feature-status--validated
                        .feature-status__header
                            i.feature-status__icon-title.fas.fa-clipboard-check
                            span.feature-status__title VALIDATED
                            i.feature-status__icon-expand.fas.fa-minus
                        .feature-status__content
                            .feature-status__list
                                .feature-status__list-item
                                    span.feature-status__feature-name recd_del_flg_bin (TINYINT)
                                .feature-status__list-item
                                    span.feature-status__feature-name recd_del_flg_bin (TINYINT)
                                .feature-status__list-item
                                    span.feature-status__feature-name recd_del_flg_bin (TINYINT)

                    .feature-status.feature-status--validating
                        .feature-status__header
                            i.feature-status__icon-title.fas.fa-eye
                            span.feature-status__title VALIDATING
                            i.feature-status__icon-expand.fas.fa-minus
                        .feature-status__content
                            .feature-status__list
                                .feature-status__list-item
                                    span.feature-status__feature-name recd_del_flg_bin (TINYINT)
                                .feature-status__list-item
                                    span.feature-status__feature-name recd_del_flg_bin (TINYINT)
                                .feature-status__list-item
                                    span.feature-status__feature-name recd_del_flg_bin (TINYINT)

                    .feature-status.feature-status--ready
                        .feature-status__header
                            i.feature-status__icon-title.fas.fa-candy-cane
                            span.feature-status__title READY
                            i.feature-status__icon-expand.fas.fa-minus
                        .feature-status__content
                            .feature-status__list
                                .feature-status__list-item
                                    span.feature-status__feature-name recd_del_flg_bin (TINYINT)
                                .feature-status__list-item
                                    span.feature-status__feature-name recd_del_flg_bin (TINYINT)
                                .feature-status__list-item
                                    span.feature-status__feature-name recd_del_flg_bin (TINYINT)

                    .feature-status.feature-status--marked-for-promotion
                        .feature-status__header
                            i.feature-status__icon-title.far.fa-grin-beam
                            span.feature-status__title MARKED FOR PROMOTION
                            i.feature-status__icon-expand.fas.fa-minus
                        .feature-status__content
                            .feature-status__list
                                .feature-status__list-item
                                    span.feature-status__feature-name recd_del_flg_bin (TINYINT)
                                .feature-status__list-item
                                    span.feature-status__feature-name recd_del_flg_bin (TINYINT)
                                .feature-status__list-item
                                    span.feature-status__feature-name recd_del_flg_bin (TINYINT)

                    .feature-status.feature-status--building
                        .feature-status__header
                            i.feature-status__icon-title.fas.fa-tools
                            span.feature-status__title BUILDING
                            i.feature-status__icon-expand.fas.fa-minus
                        .feature-status__content
                            .feature-status__list
                                .feature-status__list-item
                                    span.feature-status__feature-name recd_del_flg_bin (TINYINT)
                                .feature-status__list-item
                                    span.feature-status__feature-name recd_del_flg_bin (TINYINT)
                                .feature-status__list-item
                                    span.feature-status__feature-name recd_del_flg_bin (TINYINT)
                                .feature-status__list-item
                                    span.feature-status__feature-name recd_del_flg_bin (TINYINT)
                                .feature-status__list-item
                                    span.feature-status__feature-name recd_del_flg_bin (TINYINT)

                    .feature-status.feature-status--pending
                        .feature-status__header
                            i.feature-status__icon-title.far.fa-meh-blank
                            span.feature-status__title PENDING
                            i.feature-status__icon-expand.fas.fa-minus
                        .feature-status__content
                            .feature-status__list
                                .feature-status__list-item
                                    span.feature-status__feature-name recd_del_flg_bin (TINYINT)
                                .feature-status__list-item
                                    span.feature-status__feature-name recd_del_flg_bin (TINYINT)
                                .feature-status__list-item
                                    span.feature-status__feature-name recd_del_flg_bin (TINYINT)

                    each title in status_group1.concat(status_group2)
                        .feature-status
                            .feature-status__header
                                i.feature-status__icon-title.fas.fa-pencil-alt
                                span.feature-status__title= title
                                i.feature-status__icon-expand.fas.fa-minus
                            .feature-status__content
                                .feature-status__list
                                    .feature-status__list-item
                                        span.feature-status__feature-name recd_del_flg_bin (TINYINT)
                                    .feature-status__list-item
                                        span.feature-status__feature-name recd_del_flg_bin (TINYINT)
                                    .feature-status__list-item
                                        span.feature-status__feature-name recd_del_flg_bin (TINYINT)
                                    .feature-status__list-item
                                        span.feature-status__feature-name recd_del_flg_bin (TINYINT)
                                    .feature-status__list-item
                                        span.feature-status__feature-name recd_del_flg_bin (TINYINT)
            #mobile.feature-store
                .feature-store__header Feature store - mobile
                .feature-store__content
                    .row
                        .feature-status.feature-status--uncategorised
                            .feature-status__header
                                i.feature-status__icon-title.fas.fa-ghost
                                span.feature-status__title UNCATEGORISED
                                i.feature-status__icon-expand.fas.fa-minus
                            .feature-status__content
                                .feature-status__list
                                    .feature-status__list-item
                                        span.feature-status__feature-name recd_del_flg_bin (TINYINT)
                                    .feature-status__list-item
                                        span.feature-status__feature-name recd_del_flg_bin (TINYINT)
                                    .feature-status__list-item
                                        span.feature-status__feature-name recd_del_flg_bin (TINYINT)
                                    .feature-status__list-item
                                        span.feature-status__feature-name recd_del_flg_bin (TINYINT)
                                    .feature-status__list-item
                                        span.feature-status__feature-name recd_del_flg_bin (TINYINT)

                        .feature-status.feature-status--decommissioned
                            .feature-status__header
                                i.feature-status__icon-title.fas.fa-ban
                                span.feature-status__title DECOMMISSIONED
                                i.feature-status__icon-expand.fas.fa-minus
                            .feature-status__content
                                .feature-status__list
                                    .feature-status__list-item
                                        span.feature-status__feature-name recd_del_flg_bin (TINYINT)
                                    .feature-status__list-item
                                        span.feature-status__feature-name recd_del_flg_bin (TINYINT)
                                    .feature-status__list-item
                                        span.feature-status__feature-name recd_del_flg_bin (TINYINT)
                                    .feature-status__list-item
                                        span.feature-status__feature-name recd_del_flg_bin (TINYINT)
                                    .feature-status__list-item
                                        span.feature-status__feature-name recd_del_flg_bin (TINYINT)

                        .feature-status.feature-status--marked-for-decommission
                            .feature-status__header
                                i.feature-status__icon-title.far.fa-meh
                                span.feature-status__title MARKED FOR DECOMMISSION
                                i.feature-status__icon-expand.fas.fa-minus
                            .feature-status__content
                                .feature-status__list
                                    .feature-status__list-item
                                        span.feature-status__feature-name recd_del_flg_bin (TINYINT)
                                    .feature-status__list-item
                                        span.feature-status__feature-name recd_del_flg_bin (TINYINT)
                                    .feature-status__list-item
                                        span.feature-status__feature-name recd_del_flg_bin (TINYINT)
                                    .feature-status__list-item
                                        span.feature-status__feature-name recd_del_flg_bin (TINYINT)
                                    .feature-status__list-item
                                        span.feature-status__feature-name recd_del_flg_bin (TINYINT)

                        .feature-status.feature-status--decommissioning
                            .feature-status__header
                                i.feature-status__icon-title.fas.fa-eye-slash
                                span.feature-status__title DECOMMISSIONING
                                i.feature-status__icon-expand.fas.fa-minus
                            .feature-status__content
                                .feature-status__list
                                    .feature-status__list-item
                                        span.feature-status__feature-name recd_del_flg_bin (TINYINT)
                                    .feature-status__list-item
                                        span.feature-status__feature-name recd_del_flg_bin (TINYINT)
                                    .feature-status__list-item
                                        span.feature-status__feature-name recd_del_flg_bin (TINYINT)
                                    .feature-status__list-item
                                        span.feature-status__feature-name recd_del_flg_bin (TINYINT)
                                    .feature-status__list-item
                                        span.feature-status__feature-name recd_del_flg_bin (TINYINT)
        include _feature-panel.pug
