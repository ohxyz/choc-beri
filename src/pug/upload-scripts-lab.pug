extends _layout.pug

block link
    - var path = 'upload-scripts-lab'

block main
    .main__header
        .main__zone.main__zone--lab
            .zone-icon.i.fas.fa-flask
            .zone-name Laboratory
        .main__title Upload feature scripts
        .main__breadcrumb
            span Upload Scripts - Lab
            span /
            a(href="/") Choc-BERI
    .main__content.main__content--upload-scripts
        .__vehicle
            .strip.__vehicle__lang
                .strip__title LANGUAGE
                .strip__content
                    span.strip__item SQL
                    span.strip__item MOJO
                    span.strip__item PySpark
                    // span.strip__item Python

            .strip.__vehicle__symbol
                i.__vehicle__icon.fas.fa-plus

            .strip.__vehicle__cadence
                .strip__title CADENCE
                .strip__content
                //     span.strip__item 5 mins
                //     span.strip__item 1 hr
                //     span.strip__item 24 hrs

            .strip.__vehicle__symbol
                i.__vehicle__icon.fas.fa-equals

            .strip.__vehicle__block
                .strip__title BLOCK
                .strip__content
                    i.__vehicle__icon.fas.fa-question
                    // span.strip__item Multiple
                    // span.strip__item Single
 
        include _upload-scripts-sql.pug
        include _upload-scripts-mojo.pug
        include _upload-scripts-pyspark.pug

